{
  "version": 2,
  "builds": [
    {
      "src": "connect.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/left",
      "dest": "/connect.js"
    },
    {
      "src": "/right",
      "dest": "/connect.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}